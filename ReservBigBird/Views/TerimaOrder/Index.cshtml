<script type="text/javascript">
    function ValidasiTgl() {
        var weekday = new Array(7);
        weekday[0] = "Sunday";
        weekday[1] = "Monday";
        weekday[2] = "Tuesday";
        weekday[3] = "Wednesday";
        weekday[4] = "Thursday";
        weekday[5] = "Friday";
        weekday[6] = "Saturday";

        var a = document.getElementById("tglawalpilih").value;
        var b = document.getElementById("hariawalpilih").value;
        var c = document.getElementById("jamawalpilih").value;

        var d = document.getElementById("tglakhirpilih").value;
        var e = document.getElementById("hariakhirpilih").value;
        var f = document.getElementById("jamakhirpilih").value;

        console.log(a + ", " + b);
        var dt = new Date(a);
        var dt2 = new Date(d);

        var datenoww = dt.getDate();
        var bulanNow = dt.getMonth() + 1;

        var datenowakhir = dt2.getDate();
        var bulanNowakhir = dt2.getMonth() + 1;

        if (bulanNow < 10) bulanNow = "0" + bulanNow;
        if (datenoww < 10) datenoww = "0" + datenoww;

        if (bulanNowakhir < 10) bulanNowakhir = "0" + bulanNowakhir;
        if (datenowakhir < 10) datenowakhir = "0" + datenowakhir;

        var DateSelected = dt.getFullYear() + "-" + bulanNow + "-" + datenoww;
        var DateSelected2 = dt2.getFullYear() + "-" + bulanNowakhir + "-" + datenowakhir;

        console.log(DateSelected);
        var n = weekday[dt.getDay()];
        var m = weekday[dt2.getDay()];

        if (b == n) {

            console.log("hari 1 sama");
            if (e == m) {

                document.getElementById("tgljemputmodal").value = DateSelected;
                document.getElementById("tglselesaimodal").value = DateSelected2;

                document.getElementById("jamjemputmodal").value = c;
                document.getElementById("jamslsaimodal").value = f;

                $('#myModal13').modal('toggle');
            }
            else {
                swal("Perhatikan Hari", "Hari yang Anda Pilih di Tanggal Akhir Tidak Sama", "warning");
                console.log("Hari Tidak sama 2");
            }



        }
        else {
            swal("Perhatikan Hari", "Hari yang Anda Pilih di Tanggal Awal Tidak Sama", "warning");
            console.log("hari tidak sama");
        }

    }

</script>
